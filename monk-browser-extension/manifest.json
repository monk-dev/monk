{
  "manifest_version": 2,
  "name": "Monk",
  "version": "1.0",
  "description": "Store and search all of your content",
  "browser_action": {
    "default_title": "Monk",
    "default_popup": "src/index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["src/background.ts"]
    }
  ]
}
